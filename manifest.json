{
    "manifest_version": 3,
    "name": "XaVien",
    "version": "1.0",
    "description": "Displays the current tab's URL automatically when a page is loaded.",
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}